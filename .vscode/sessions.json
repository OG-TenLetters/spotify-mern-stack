{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v11/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "Playground",
        "autoExecuteCommands": true,
        "icon": "flame",
        "color": "terminal.ansiBrightRed",
        "commands": [
          "clear; Write-Host \"Good Morning! Don't forget your water!\" -ForegroundColor Yellow"
        ]
      },
      [
        {
          "name": "Frontend",
          "autoExecuteCommands": true,
          "icon": "debug-breakpoint-log-unverified",
          "color": "terminal.ansiBlue",
          "cwd": "./frontend",
          "commands": [
            "clear; Write-Host \"Frontend Log Ready\" -ForegroundColor Blue"
          ]
        },
        {
          "name": "Frontend Server",
          "autoExecuteCommands": true,
          "icon": "debug-breakpoint-log",
          "color": "terminal.ansiBrightBlue",
          "cwd": "./frontend",
          "commands": ["npm run dev"]
        }
      ],
      [
        {
          "name": "Backend",
          "autoExecuteCommands": true,
          "icon": "circle-outline",
          "color": "terminal.ansiGreen",
          "cwd": ".\\backend",
          "commands": [
            "clear; Write-Host \"Backend Log Ready\" -ForegroundColor Green"
          ]
        },
        {
          "name": "Backend Server",
          "autoExecuteCommands": true,
          "icon": "circle-filled",
          "color": "terminal.ansiBrightGreen",
          "cwd": ".\\backend",
          "commands": ["npm run dev"]
        }
      ],
      [
        {
          "name": "Songs Data",
          "autoExecuteCommands": true,
          "icon": "music",
          "color": "terminal.ansiBrightMagenta",
          "cwd": ".\\backend",
          "commands": [""]
        },
        {
          "name": "Albums Data",
          "autoExecuteCommands": true,
          "icon": "layout-activitybar-left",
          "color": "terminal.ansiBrightMagenta",
          "cwd": ".\\backend",
          "commands": [""]
        }
      ]
    ],
    "saved-session": [
      {
        "name": "connect",
        "commands": [""]
      }
    ]
  }
}
